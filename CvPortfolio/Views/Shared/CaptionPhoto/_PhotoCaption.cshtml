@model CaptionViewModel

<img src="~/Uploads/@Model.Photo.Photo" alt=" image" class="jarallax-img">
<div id="heroparticles"></div>
<div class="container h-100">
    <div class="row h-100 align-items-center">
        <div class="col-lg-9 col-md-12 col-sm-12">
            <div class="hero-inner white-text">
                <div class="hero-social-white">
                    @foreach (var item in Model.Icons)
                    {
                        <a href="@item.EndPoint"><i class="@item.Logo"></i></a>
                    }

                </div>
                <h1>Im <span id="typed-text" dato="@Model.Title"></span></h1>
                <p>
                    @Model.Detail
                </p>
                <a href="#" class="default-white-btn">Hire Me <i class="ml-2 fas fa-thumbs-up"></i></a>
            </div>
        </div>
    </div>
</div>
<div class="button-group-center wow zoomIn" data-wow-delay="0.4s">
    <a href="#" class="scroll-down-btn" data-scroll-nav="2">
        <i class="fas fa-angle-double-down"></i>
    </a>
    <a href="https://www.youtube.com/watch?v=mQFI37HH_eY" class="popup-youtube my-video-btn">
        <span class="fa fa-play"></span>
    </a>
</div>

